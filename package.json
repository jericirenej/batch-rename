    "compile": "tsc",
    "ts-exec": "npx ts-node",
    "ts-run": "npx ts-node src/index.ts",
    "compileAll": "npm run precompile && tsc packages/cli/"
  },
  "author": "Irenej Jeriƒç",
  "license": "ISC",
  "bin": "dist/index.js",
  "type": "module",
  "devDependencies": {
    "@typescript-eslint/parser": "^5.57.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.14.2",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "eslint": "^8.37.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.25.2",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "prettier": "^2.8.4",
